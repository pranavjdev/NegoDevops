<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>NegociosDevops</display-name>
  <welcome-file-list>
    <welcome-file>Login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LoginController</servlet-name>
    <servlet-class>com.negociosit.negociosdevops.LoginController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TimeSheetController</servlet-name>
    <servlet-class>com.negociosit.negociosdevops.TimeSheetController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Home</servlet-name>
    <servlet-class>com.negociosit.negociosdevops.HomeController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Scrum</servlet-name>
    <servlet-class>com.negociosit.negociosdevops.ScrumSenderController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ChangePassword</servlet-name>
    <servlet-class>com.negociosit.negociosdevops.ChangePasswordController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LeaveRequestController</servlet-name>
    <servlet-class>com.negociosit.negociosdevops.LeaveRequestController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginController</servlet-name>
    <url-pattern>/loginController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Home</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Scrum</servlet-name>
    <url-pattern>/scrum</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TimeSheetController</servlet-name>
    <url-pattern>/timesheet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ChangePassword</servlet-name>
    <url-pattern>/changepassword</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LeaveRequestController</servlet-name>
    <url-pattern>/leave</url-pattern>
  </servlet-mapping>
</web-app>